exports[`archive 1`] = `
"systemata archive, version 0.0.1
  Create a static, versioned copy of the current design system.

Usage:
  systemata archive [options]

Global options:
  -h, --help       Print usage information
  -v, --version    Print systemata version

'archive' options:
  -c, --config <configFile>   Path to config file"
`;

exports[`build 1`] = `
