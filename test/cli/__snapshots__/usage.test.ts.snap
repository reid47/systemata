exports[`init 1`] = `
"systematic init, version 0.0.1
  Creates a new system config file.

Usage:
  systematic init [options]

Global options:
  -h, --help       Print usage information
  -v, --version    Print systematic version

'init' options:
  -c, --config <configFile>   Path to config file
  -y, --yes                   Skip questions, use defaults"
`;

exports[`main 1`] = `
"systematic, version 0.0.1
  Build a design system with one file

Usage:
  systematic <command> [options]

Global options:
  -h, --help       Print usage information
  -v, --version    Print systematic version

Commands:
  archive          Archive a static copy of current design system
  build            Build all design system files
  develop          Start a build server & re-build on change
  init             Create a new design system

For help with a command, use 'systematic <command> -h'."
`;

exports[`show-config 1`] = `
"systematic show-config, version 0.0.1
  Prints resolved system config as systematic sees it. Useful for making sure configuration looks as you expect.

Usage:
  systematic show-config [options]

Global options:
  -h, --help       Print usage information
  -v, --version    Print systematic version

'show-config' options:
  -c, --config <configFile>   Path to config file"
`;
