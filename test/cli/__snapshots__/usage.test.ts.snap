exports[`archive 1`] = `
"systematic archive, version 0.0.1
  Create a static, versioned copy of the current design system.

Usage:
  systematic archive [options]

Global options:
  -h, --help       Print usage information
  -v, --version    Print systematic version

'archive' options:
  -c, --config <configFile>   Path to config file"
`;

exports[`build 1`] = `
